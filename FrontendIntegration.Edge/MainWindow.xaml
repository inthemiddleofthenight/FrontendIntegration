<Window x:Class="FrontendIntegration.Edge.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FrontendIntegration.Edge"
        mc:Ignorable="d"
        Title="MainWindow Edge Frontend integration" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">
            <Button Margin="4" Click="Button_Click">Call JS</Button>
        </StackPanel>
        <Grid x:Name="Container" Grid.Column="1">

        </Grid>
    </Grid>
</Window>
